    {
      "cell_type": "code",
      "source": [
        "# Step 3：安裝套件（只需執行一次）\n",
        "!pip install -U transformers\n",
        "!pip install torch\n"
      ],
      "metadata": {
        "id": "CXXwoHeclfwr"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Step 4：載入模型並定義預測函式\n",
        "from transformers import BertTokenizer, BertForSequenceClassification\n",
        "import torch\n",
        "\n",
        "# 載入模型與分詞器\n",
        "model = BertForSequenceClassification.from_pretrained(\"./results/results/checkpoint-300\")\n",
        "tokenizer = BertTokenizer.from_pretrained(\"bert-base-chinese\")\n",
        "\n",
        "# 預測函式\n",
        "def predict(text):\n",
        "    inputs = tokenizer(text, return_tensors=\"pt\", truncation=True, padding=True)\n",
        "    with torch.no_grad():\n",
        "        outputs = model(**inputs)\n",
        "    probs = torch.nn.functional.softmax(outputs.logits, dim=-1)\n",
        "    label = torch.argmax(probs).item()\n",
        "    confidence = probs[0][label].item()\n",
        "    return \"詐騙留言\" if label == 1 else \"正常留言\", confidence\n",
        "\n"
      ],
      "metadata": {
        "id": "nJSRmZx_lk6x"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Step 5：輸入你要測試的留言\n",
        "text = \"出售PS5主機\"\n",
        "result, score = predict(text)\n",
        "print(f\"判斷結果：{result}（信心分數：{score:.2f}）\")"
      ],
      "metadata": {
        "id": "dHsXbpZWPJqH"
      },
      "execution_count": null,
      "outputs": []
    }
  ]
